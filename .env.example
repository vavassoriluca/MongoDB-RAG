# .env
# Rename this file to .env and fill in your credentials.

# Your MongoDB Atlas connection string.
# Make sure the user has read/write permissions on the database.
# Example: mongodb+srv://<user>:<password>@<cluster-uri>/?retryWrites=true&w=majority
MONGO_URI="mongodb+srv://.mongodb.net/?retryWrites=true&w=majority"

# Your Voyage AI API Key.
# Get it from https://dash.voyageai.com/api-keys
VOYAGE_API_KEY="..."

# The host for your local Ollama instance.
# This is the default value. Change it if your Ollama server is running elsewhere.
OLLAMA_HOST="http://localhost:11434"